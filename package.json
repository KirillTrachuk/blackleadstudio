{
  "name": "nesh-website",
  "version": "1.0.0",
  "description": "Static website",
  "private": true,
  "main": "./app/js/main.js",
  "engines": {
    "yarn": "^1.17",
    "node": "^10.15 || ^12.8"
  },
  "scripts": {
    "start": "node server/index.js",
    "dev": "webpack-dev-server --host 0.0.0.0 --mode development --color --env.noclear --env.hmr",
    "build": "webpack --display-error-details --mode development",
    "build:minify": "yarn build --env.fullminify",
    "build:prod": "webpack --display-error-details --mode production",
    "build:prod:minify": "yarn build:prod --env.fullminify",
    "build:prod:minify:env": "cross-env NODE_ENV=production yarn build:prod:minify",
    "heroku-postbuild": "yarn build:prod:minify && cd server && yarn",
    "build:prod:minify:env:toserver": "yarn build:prod:minify:env --env.output ../public/web"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.3",
    "@babel/runtime": "^7.12.5",
    "core-js": "^3",
    "detect-browser": "^4.8.0",
    "exports-loader": "^0.7.0",
    "gsap": "./gsap-member/npm-install-this/gsap-bonus.tgz",
    "hammerjs": "^2.0.8",
    "lethargy": "^1.0.9",
    "lottie-web": "^5.6.3",
    "mobx": "^5.15.4",
    "plyr": "^3.5.6",
    "regenerator-runtime": "^0.13.3",
    "reset-css": "^5.0.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@types/gsap": "^1.20.2",
    "autoprefixer": "^9.7.4",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-minify-webpack-plugin": "^0.3.1",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.0",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.18.3",
    "file-loader": "^5.0.2",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^4.0.0-beta.8",
    "image-webpack-loader": "^6.0.0",
    "markdown-loader": "^5.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "raw-loader": "^4",
    "resolve-url-loader": "^3.1.1",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.1.3",
    "underscore-template-loader": "^1.0.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.2"
  }
}
